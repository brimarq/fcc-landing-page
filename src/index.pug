        img(src="https://storage.googleapis.com/coverr-public/poster/Strum-Away.jpg" alt="")

  
section#section1
  p Section 1

  .testimonial-container
    - for (let i = 0; i < 7; i++)
      .testimonial
        .testimonial-img
          .logo-soundhole(class=`testimonial-${i}`)
        .testimonial-quote
          p #{testimonials[i].quote}
          p #{testimonials[i].author}

section#section2
  p Section 2

  .product-cards
    - for (var x = 0; x < 3; x++)
      .card
        .card-header: p classic strings
        .card-logo
          .logo-soundhole
        .card-footer: p one set


