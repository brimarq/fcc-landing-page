  svg(xmlns='http://www.w3.org/2000/svg' viewBox='0 0 512 512' preserveAspectRatio='xMidYMin slice' style='display:block;box-sizing:content-box;height:0.1px;width:100%;padding-bottom:100%;overflow:visible;')
    use(href='#svg-card-logo' x='62.119' y='118.706')
    use(href='#svg-card-header' x='95.413' y='27.992')
    use(href=`#svg-card-footer-${tension}` x='134.227' y='463.684')

