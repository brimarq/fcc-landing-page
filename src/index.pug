mixin svgLogoFull() 
  -
    const logo_full = {
      h: 100,
      space: 10,
      symbol: {
        logo: { h: 341.794, w: 390.12 },
        text: { h: 84.714, w: 321.173 }
      },
      get logo_h() { return this.h * 1 },
      get text_h() { return this.h * .75 },
      get logo_w() { return this.logo_h / this.symbol.logo.h * this.symbol.logo.w },
      get text_w() { return this.text_h / this.symbol.text.h * this.symbol.text.w },
      get logo_y() { return (this.h - this.logo_h) / 2 },
      get text_x() { return this.logo_w + this.space },
      get text_y() { return (this.h - this.text_h) / 2 },
      get w() { return this.logo_w + this.space + this.text_w },
      get padding_ratio() { return this.h / this.w }
    }
  svg(
    xmlns='http://www.w3.org/2000/svg' 
    viewBox=`0 0 ${logo_full.w} ${logo_full.h}` 
    preserveAspectRatio='xMidYMin slice' 
    style=`display:block;box-sizing:content-box;height:0.1px;width:100%;padding-bottom:${logo_full.padding_ratio * 100}%;overflow:visible;`
  )
    use(href='#svg-card-logo' x='0' y=`${logo_full.logo_y}` width=`${logo_full.logo_w}` height=`${logo_full.logo_h}`)
    use(href='#svg-card-header' x=`${logo_full.text_x}` y=`${logo_full.text_y}` width=`${logo_full.text_w}` height=`${logo_full.text_h}`)

