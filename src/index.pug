header#header
  
  nav#nav-bar

    .nav-bar-left
      a(href="#")
        img(src='https://via.placeholder.com/40' style='display: none;')
        +svgLogo('40', 'white', 'white', '10', true)
  
    .nav-bar-right
      ul
        li: a.nav-link(href="#section1") Section 1 
        li: a.nav-link(href="#section2") Section 2
        li: a.nav-link(href="#section3") Section 3

  .hero-video
    .video-container
      video#video(autoplay loop muted playsinline) 
        source(src="https://storage.googleapis.com/coverr-main/webm/Strum-Away.webm" type="video/webm")
        source(src="https://storage.googleapis.com/coverr-main/ogv/Strum-Away.ogv" type="video/ogg")
        source(src="https://storage.googleapis.com/coverr-main/mp4/Strum-Away.mp4" type="video/mp4")
